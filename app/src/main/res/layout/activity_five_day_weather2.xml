         />

    <TextView
        android:id="@+id/five_day_weather_textview4"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="20dp"
        android:textSize="20sp"
        />

    <TextView
        android:id="@+id/five_day_weather_textview5"
        android:layout_width="wrap_content"
